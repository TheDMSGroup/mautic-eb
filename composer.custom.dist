{
    "require": {
      "heathdutton/mautic-eb-custom": "dev-master",
      "heathdutton/mautic-extended-fields": "dev-master",
      "heathdutton/mautic-contact-client": "dev-master"
    },
    "repositories": [
      {
        "type": "package",
        "package": {
          "type": "library",
          "name": "heathdutton/mautic-eb-custom",
          "version": "dev-master",
          "source": {
            "type": "git",
            "url": "https://github.com/heathdutton/mautic-eb-custom.git",
            "reference": "master"
          },
          "require": {
            "mnsami/composer-custom-directory-installer": "1.1.*"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "type": "library",
          "name": "heathdutton/mautic-extended-fields",
          "version": "dev-master",
          "source": {
            "type": "git",
            "url": "https://github.com/heathdutton/mautic-extended-fields.git",
            "reference": "master"
          },
          "require": {
            "heathdutton/mautic-eb-custom": "dev-master",
            "mautic/core": "dev-master",
            "mnsami/composer-custom-directory-installer": "1.1.*"
          }
        }
      },
      {
        "type": "package",
        "package": {
          "type": "library",
          "name": "heathdutton/mautic-contact-client",
          "version": "dev-master",
          "source": {
            "type": "git",
            "url": "https://github.com/heathdutton/mautic-contact-client.git",
            "reference": "master"
          },
          "require": {
            "heathdutton/mautic-eb-custom": "dev-master",
            "mautic/core": "dev-master",
            "mnsami/composer-custom-directory-installer": "1.1.*"
          }
        }
      }
    ],
    "extra": {
        "installer-paths": {
            "./mautic_custom": [
                "heathdutton/mautic-eb-custom"
            ],
            "./mautic/plugins/MauticExtendedFieldsBundle": [
                "heathdutton/mautic-extended-fields"
            ],
            "./mautic/plugins/MauticContactClientBundle": [
                "heathdutton/mautic-contact-client"
            ]
        }
    }
}
