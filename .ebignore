# Similar to a .gitignore, this describes what to ignore when zipping up a release for Elastic Beanstalk.
.*
!.gitignore
!.ebignore
!.htaccess
# !.gitkeep
!.travis.yml
!/.ebextensions
!/.ebextensions/*
!/.github
/composer.phar

# Projects not invented here.
# /mautic
/mautic/bower_components
/mautic/node_modules
/mautic/npm-debug.log
# /vendor
# /bin
/selenium
# /mautic_custom/*
# !/mautic_custom/README.md
# /composer.custom

# Elastic Beanstalk Files
/.elasticbeanstalk/*
!/.elasticbeanstalk/*.cfg.yml
!/.elasticbeanstalk/*.global.yml

/mautic/app/bootstrap*
/mautic/app/tests.bootstrap*
/mautic/app/phpunit.xml

/mautic/app/cache

/mautic/app/config/local*.php
/mautic/app/config/config_local.php
!/mautic/app/config/paths_local.php
!/mautic/app/config/parameters_local.php

/mautic/app/logs

/mautic/app/Resources/SensioGeneratorBundle
/mautic/app/spool/*

/mautic/bundles
/mautic/build/coverage
/mautic/build/packaging
/mautic/build/packages/*
!/mautic/build/packages/.placeholder
/mautic/mockup
/mautic/upgrade
/mautic/upgrade_errors.txt

/mautic/media/dashboards/*
